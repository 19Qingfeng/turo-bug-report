{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["test", "lint"],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    },
    "dev": {
      "cache": false
    }
  }
}